Dialogue: 0,0:57:02.18,0:57:03.49,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:04.08,0:57:05.68,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:06.40,0:57:07.50,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:10.42,0:57:12.52,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:13.16,0:57:15.00,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:15.40,0:57:17.20,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:20.00,0:57:21.21,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:22.66,0:57:24.60,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:32.99,0:57:34.41,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:35.17,0:57:36.39,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:37.80,0:57:38.82,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:39.78,0:57:40.98,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:43.36,0:57:44.08,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:51.29,0:57:52.10,爱丽丝,,0,0,0,,
Dialogue: 0,0:57:53.30,0:57:54.71,爱丽丝,,0,0,0,,
Dialogue: 0,0:58:08.79,0:58:10.80,爱丽丝,,0,0,0,,
